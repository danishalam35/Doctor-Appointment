<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.callmydoctors">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AllActivity.OnlineConsultationActivity"></activity>
        <activity android:name=".AllActivity.DiagnosticTestActivity" />
        <activity android:name=".AllActivity.DiagnosticsActivity" />
        <activity android:name=".AllActivity.PaymentActivity" />
        <activity android:name=".AllActivity.MeetingActivity" />
        <activity android:name=".AllActivity.BookAnAppointmentActivity" />
        <activity android:name=".AllActivity.LoginActivity" />
        <activity android:name=".AllActivity.NamesActivity" />
        <activity android:name=".AllActivity.VerificationActivity" />
        <activity android:name=".AllActivity.DashboardActivity" />
        <activity android:name=".AllActivity.DoctorsActivity" />
        <activity android:name=".AllActivity.MainActivity" />
        <activity android:name=".AllActivity.IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>